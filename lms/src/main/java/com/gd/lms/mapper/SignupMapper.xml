<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gd.lms.mapper.SignupMapper">

	<!-- 교수추가 (회원가입) -->
	<insert id="insertProfessor" parameterType="com.gd.lms.vo.Professor">
	INSERT INTO professor (professor_no,professor_pass,major_no,professor_name,professor_regi_no,professor_age,professor_gender,professor_telephone,professor_email,professor_address,professor_detail_address)
	VALUES (#{professorNo},#{professorPass},#{majorNo},#{professorName},#{professorRegiNo},#{professorAge},#{professorGender},#{professorTelephone},#{professorEmail},#{professorAddress},#{professorDetailAddress})
	</insert>

</mapper>